# データベース設定
DATABASE_URL=postgresql://username:password@localhost:5432/epub_reader

# Cloudflare R2 ストレージ設定
R2_ACCESS_KEY_ID=your_access_key_id
R2_SECRET_ACCESS_KEY=your_secret_access_key
